{
  "name": "suds-code",
  "displayName": "SUDS for VSCode",
  "description": "Support for SUDS (Steve's Unreal Dialogue System) script",
  "version": "1.0.1",
  "publisher": "sstreeting",
  "icon": "img/Icon128.png",
  "keywords": ["suds", "unreal", "ue5", "dialogue"],
  "repository": {
    "type": "Git",
    "url": "https://github.com/sinbad/suds-code.git"
  },
  "engines": {
    "vscode": "^1.72.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "suds",
      "aliases": ["SUDS", "suds"],
      "extensions": [".sud"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "suds",
      "scopeName": "source.sud",
      "path": "./syntaxes/suds.tmLanguage.json"
    }]
  }
}
